<script>
    function toggleDarkMode() {
        halfmoon.toggleDarkMode();

        let id = document.querySelector("#darkmode-toggle");

        if (halfmoon.darkModeOn) {
            id.classList.replace("fa-moon", "fa-sun")
        } else {
            id.classList.replace("fa-sun", "fa-moon")
        }
    }

    toggleDarkMode()
</script>